# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Portfolio Username
# The username of the portfolio to display
NEXT_PUBLIC_USERNAME=your_username_here

# PSK Token for Portfolio Access
# This token is used to authenticate with the backend API
# Get this token from your portfolio settings or backend admin
NEXT_PUBLIC_PSK_TOKEN=your_psk_token_here
